<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate='1'>
    <!-- sequence for SPPD -->
    <record model="ir.sequence" id="seq_sppd">
      <field name="name">Pengajuan SPPD</field>
      <field name="code">sppd</field>
      <field name="prefix">SPPD</field>
      <field name="padding">4</field>
      <field name="company_id" eval="False"/>
    </record>

    <record model="ir.sequence" id="seq_lphd">
      <field name="name">LPHD</field>
      <field name="code">lphd</field>
      <field name="prefix">LPHD</field>
      <field name="padding">4</field>
      <field name="company_id" eval="False"/>
    </record>
  </data>
</odoo>
